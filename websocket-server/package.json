{
  "name": "dnd-quiz-websocket-server",
  "version": "2.0.0",
  "description": "WebSocket server for DND Live Quiz plugin with Redis backend",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "prod": "pm2 start ecosystem.config.js",
    "stop": "pm2 stop ecosystem.config.js",
    "restart": "pm2 restart ecosystem.config.js",
    "logs": "pm2 logs dnd-quiz-ws",
    "monit": "pm2 monit",
    "test": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "websocket",
